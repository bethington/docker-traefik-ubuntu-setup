# User and system settings
PUID=1000
PGID=1000
TZ=America/Chicago

# Cloudflare API tokens (create at https://dash.cloudflare.com/profile/api-tokens)
CF_DNS_API_TOKEN=your-cloudflare-dns-token-here
CF_ZONE_API_TOKEN=your-cloudflare-zone-token-here

# GoDaddy API credentials (create at https://developer.godaddy.com/keys)
GODADDY_API_KEY=your-godaddy-api-key
GODADDY_API_SECRET=your-godaddy-api-secret

# Database credentials
MYSQL_USER=your-mysql-user
MYSQL_PASSWORD=your-secure-password
TIKI_DB_USER=tiki
TIKI_DB_PASS=your-tiki-password
PASS=your-default-password

# pgAdmin configuration
PGADMIN_EMAIL=your-email@example.com

# SimpleLogin configuration
SIMPLELOGIN_DB_PASSWORD=your-simplelogin-db-password
SIMPLELOGIN_SECRET=generate-with-openssl-rand-hex-32
SIMPLELOGIN_HOSTNAME=mail.yourdomain.com
